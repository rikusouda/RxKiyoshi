
def shared_pods
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RealmSwift'
    pod 'MBProgressHUD', '~> 1.0.0'
end

target 'RxKiyoshi' do
    platform :ios, '10.0'
    use_frameworks!
    inhibit_all_warnings!
    shared_pods
end

target 'RxKiyoshiTests' do
    platform :ios, '10.0'
    use_frameworks!
    inhibit_all_warnings!
    shared_pods
end
